<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Zenify PHP">
  <template name="d" value="dump($END$);" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="st" value="declare(strict_types=1);" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="try" value="try {&#10;&#9;$SELECTION$&#10;&#10;} catch ($EXCEPTION_TYPE$ $e) {&#10;&#9;$END$&#10;}&#10;" description="try {} catch () {}" toReformat="true" toShortenFQNames="true">
    <variable name="EXCEPTION_TYPE" expression="classNameComplete()" defaultValue="Exception" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pro" value="/***/&#10;protected function $NAME$($ARGS$)&#10;{&#10;    $END$&#10;}" description="protected function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pri" value="/***/&#10;private function $NAME$($ARGS$)&#10;{&#10;    ctor$END$&#10;}" description="private function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="pub" value="public function $NAME$($ARGS$)&#10;{&#10;    $END$&#10;}" description="public function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ctor" value="public function __construct($END$)&#10;{&#10;}" description="constructor" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
</templateSet>
