<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Zenify Latte">
  <template name="a" value="&lt;a n:href=&quot;$PRESENTER$:$ACTION$&quot;&gt;$NODE$&lt;/a&gt;" description="href" toReformat="true" toShortenFQNames="true">
    <variable name="PRESENTER" expression="" defaultValue="&quot;Homepage&quot;" alwaysStopAt="true" />
    <variable name="ACTION" expression="" defaultValue="&quot;default&quot;" alwaysStopAt="true" />
    <variable name="NODE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="SQL" value="false" />
      <option name="JSP" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="_" value="{_$VAR$}" description="translate" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="SQL" value="false" />
      <option name="JSP" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="uli" value="{if $user-&gt;loggedIn}&#10;&#9;$END$        &#10;{/}" description="if user is logged in" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="false" />
      <option name="XSL_TEXT" value="false" />
      <option name="XML" value="false" />
      <option name="SQL" value="false" />
      <option name="JSP" value="false" />
      <option name="CSS_PROPERTY_VALUE" value="false" />
      <option name="CSS_DECLARATION_BLOCK" value="false" />
      <option name="CSS_RULESET_LIST" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="TypeScript" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="PHP" value="false" />
      <option name="HAML" value="false" />
      <option name="OTHER" value="false" />
    </context>
  </template>
  <template name="fe" value="{foreach $$$RESULT$ as $$$ROW$}&#10;&#9;{$$$ROW$-&gt;$END$}&#10;{/}" description="foreach" toReformat="false" toShortenFQNames="true">
    <variable name="RESULT" expression="" defaultValue="&quot;result&quot;" alwaysStopAt="true" />
    <variable name="ROW" expression="" defaultValue="&quot;row&quot;" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="HTML" value="true" />
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>

